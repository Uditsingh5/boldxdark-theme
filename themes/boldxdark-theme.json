    "tokenColors": [
      {
        "scope": "comment",
        "settings": {
          "foreground": "#6A737D",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#D16DFF",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["variable", "identifier"],
        "settings": {
          "foreground": "#DCE0E8"
        }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#4F83E3",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.name.type", "support.class"],
        "settings": {
          "foreground": "#F0B86B",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["constant", "constant.numeric"],
        "settings": {
          "foreground": "#E6A157"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#B6D77A"
        }
      },
      {
        "scope": ["keyword.operator", "punctuation"],
        "settings": {
          "foreground": "#C0C6D0"
        }
      },
      {
        "scope": ["meta.decorator", "entity.other.attribute-name"],
        "settings": {
          "foreground": "#FF8C8C"
        }
      },
      {
        "scope": ["invalid", "markup.error"],
        "settings": {
          "foreground": "#FF5C5C",
          "fontStyle": "bold underline"
        }
      }
    ]
  }
  